{
  "name": "world-check-map",
  "version": "1.0.0",
  "description": "Mark countries you have already been to",
  "scripts": {
    "start": "dotnet run",
    "build": "npm-run-all --parallel build:*",
    "build:client": "webpack",
    "build:server": "dotnet build",
    "test": "npm-run-all --parallel test:*",
    "test:server": "npm-run-all --sequential build:server --parallel tests:*",
    "tests:unit": "dotnet test --no-build ../WorldCheckMap.Tests.Unit/WorldCheckMap.Tests.Unit.csproj",
    "tests:integration": "dotnet test --no-build ../WorldCheckMap.Tests.Integration/WorldCheckMap.Tests.Integration.csproj"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RomanFrom710/WorldCheckMap.git"
  },
  "author": "Roman Chaley",
  "license": "ISC",
  "homepage": "https://github.com/RomanFrom710/WorldCheckMap",
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^3.3.7",
    "history": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-simple-maps": "^0.9.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "aspnet-prerendering": "^3.0.1",
    "aspnet-webpack": "^2.0.1",
    "aspnet-webpack-react": "^3.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "jsx-loader": "^0.13.2",
    "less": "^3.0.0-alpha.3",
    "less-loader": "^4.0.5",
    "react-hot-loader": "^3.1.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-hot-middleware": "^2.20.0"
  }
}
